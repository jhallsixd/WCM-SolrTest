        <reference name="staticConfigManagerFactory" interface="com.st.olm.cq.config.StaticConfigManagerFactory" cardinality="1..1" policy="static" bind="bindStaticConfigManagerFactory" unbind="unbindStaticConfigManagerFactory"/>
